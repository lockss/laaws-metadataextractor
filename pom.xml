    <failOnMissingWebXml>false</failOnMissingWebXml>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <slf4j-version>1.7.18</slf4j-version>
    <version.resteasy>3.0.19.Final</version.resteasy>
